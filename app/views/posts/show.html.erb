<div class='container'>
<div class='jumbotron' >
  <h3><%= @post.title %></h3>
  <%= link_to 'Edit post', edit_post_path(@post.id) if user_signed_in? %> 
  <p><%= @post.body %></p>

  <h6>Created on <%= @post.created_at %></h6>
  <h6>Last edited <%= @post.updated_at %></h6>
  <h6>Posted by <%= mail_to 'bcgoss@gmail.com', 'Bryan Goss' %></h6>
</div>
</div>
